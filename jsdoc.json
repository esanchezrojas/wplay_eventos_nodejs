{
    "plugins": [],
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.js$",
        "excludePattern": "((^|\\/|\\\\)_)"
    },

    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        " recurse": true
    }
}